<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.honeytips">
    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS"/>
    <uses-permission android:name="android.permission.READ_PROFILE"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE">
    </uses-permission>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
            android:allowBackup="true"
            android:icon="@drawable/icon"
            android:label="꿀팁 저장소"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".FindPassword">
        </activity>
        <activity android:name=".SignInActivity">
        </activity>
        <activity android:name=".LoginActivity">
        </activity>
        <activity android:name=".Favorite_Read">
        </activity>
        <activity android:name=".Favorite">
        </activity>
        <activity android:name=".Update">
        </activity>
        <activity android:name=".ReadSelect">
        </activity>
        <activity android:name=".Read">
        </activity>
        <activity android:name=".Write">
        </activity>
        <activity android:name=".Menu">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>